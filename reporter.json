{
    "problemMatcher": [
        {
            "owner": "check-spelling",
            "pattern": [
                {
                    "regexp": "^(.+):(\\d+):(\\d+) \\.{3} (\\d+),\\s(Error|Warning|Notice)\\s-\\s(.+\\s\\((.+)\\))$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "endColumn": 4,
                    "severity": 5,
                    "message": 6,
                    "code": 7
                }
            ]
        }
    ]
}

